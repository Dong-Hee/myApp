<form action="/board/form/reg" method="post" enctype="multipart/form-data">
    <div class="grid-container">
        <div class="grid-x grid-padding-x">
            <div class="medium-6 cell">
                <label>제목
                    <input type="text" placeholder="" id="subject" name="subject">
                </label>
            </div>
            <div class="medium-6 cell">대표이미지
                <input class="button" type="file" id="img" name="img">
                <img id="preview" src="" />
            </div>
            <div class="medium-3 cell">
                <label>즉구가
                    <input type="text" placeholder="" id="buyNow" name="buyNow">
                </label>
            </div>
            <div class="medium-3 cell">
                <label>입찰 시작가 
                    <input type="text" placeholder="" id="startPrice" name="startPrice">
                </label>
            </div>
            <div class="medium-3 cell">
                <label>마감시간
                    <input type="datetime-local" placeholder="" id="endDate" name="endDate">
                </label>
            </div>
            <div class="medium-6 cell">
                <label>
                    description
                <textarea placeholder="None" rows="10" id="description" name="description"></textarea>
                </label>
            </div>
        </div>
    </div>
    <button type="submit" class="success button" id="form-submit">Save</button>
</form>

